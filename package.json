    "@next/eslint-plugin-next": "^15.5.5",
    "@playwright/test": "^1.56.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/dom-chromium-ai": "0.0.6",
    "@types/dompurify": "^3.2.0",
    "@types/gif.js": "^0.2.5",
    "@types/ini": "^4.1.1",
    "@types/jest": "^30.0.0",
