{
  "name": "daedalos",
  "version": "2.0.0",
  "description": "Desktop environment in the browser",
  "author": {
    "name": "Dustin Brett",
    "email": "dustinbrett@gmail.com",
    "url": "https://dustinbrett.com"
  },
  "license": "MIT",
  "browserslist": [
    "defaults and supports es6 and not ie 11 and >1%"
  ],
  "scripts": {
    "build": "yarn build:prebuild && next build",
    "build:bundle-analyzer": "yarn build",
    "build:fs": "node scripts/fs2json.js --exclude .index --out public/.index/fs.9p.json ./public && node scripts/preloadIcons.js",
    "build:html": "node scripts/minifyHtml.js",
    "build:js": "node scripts/minifyJs.js",
    "build:prebuild": "yarn build:fs && yarn build:search",
    "build:robots": "node scripts/robots.js",
    "build:search": "node scripts/searchIndex.js",
    "deploy": "yarn build:robots && next export && yarn build:html && yarn build:js",
    "docker:build": "docker build -t daedalos .",
    "docker:run": "docker run -dp 3000:3000 --rm --name daedalos daedalos",
    "dev": "next dev",
    "eslint": "eslint --report-unused-disable-directives .",
    "export": "next export",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "start": "next build && next start",
    "stylelint": "stylelint --formatter=verbose **/*.ts*",
    "test": "jest",
    "test:watch": "jest --watch",
    "unused-exports": "ts-prune --project tsconfig.json --ignore \\pages --error"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "stylelint --fix",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.2",
    "@monaco-editor/react": "^4.5.1",
    "@panzoom/panzoom": "^4.5.1",
    "@prettier/plugin-xml": "^2.2.0",
    "@wasmer/wasm-terminal": "^0.12.0",
    "@wasmer/wasm-transformer": "^0.12.0",
    "ani-cursor": "^0.0.5",
    "browserfs": "https://github.com/jvilk/BrowserFS.git#a96aa2d",
    "butterchurn-presets": "^3.0.0-beta.4",
    "fflate": "^0.8.0",
    "file-type": "^18.5.0",
    "fix-webm-duration": "^1.0.5",
    "framer-motion": "^10.12.18",
    "gif.js": "^0.2.0",
    "idb": "^7.1.1",
    "ini": "^4.1.1",
    "isomorphic-git": "^1.24.2",
    "minimist": "^1.2.8",
    "multiformats": "^12.0.1",
    "music-metadata-browser": "^2.5.10",
    "next": "^13.4.8",
    "opentype.js": "^1.3.4",
    "playlist-parser": "^0.0.12",
    "prettier": "^2.8.8",
    "print-js": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-rnd": "^10.4.1",
    "resedit": "^2.0.0",
    "rtf.js": "^3.0.9",
    "styled-components": "^6.0.2",
    "utif": "https://github.com/photopea/UTIF.js",
    "wasi-js": "^1.7.3"
  },
  "devDependencies": {
    "7z-wasm": "^1.0.2",
    "@next/bundle-analyzer": "^13.4.8",
    "@next/eslint-plugin-next": "^13.4.8",
    "@testing-library/dom": "^9.3.1",
    "@types/gif.js": "^0.2.2",
    "@types/ini": "^1.3.31",
    "@types/jest": "^29.5.2",
    "@types/lunr": "^2.3.4",
    "@types/node": "^20.3.3",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/opentype.js": "^1.3.4",
    "@types/react": "^18.2.14",
    "@types/ua-parser-js": "^0.7.36",
    "@types/video.js": "^7.3.52",
    "@types/wicg-file-system-access": "^2020.9.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "emulators": "^0.73.8",
    "emulators-ui": "^0.73.9",
    "eruda": "^3.0.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-minifier-terser": "^7.2.0",
    "html-to-image": "^1.11.11",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "lint-staged": "^13.2.3",
    "lunr": "^2.3.9",
    "monaco-editor": "^0.39.0",
    "pdfjs-dist": "^3.8.162",
    "postcss-styled-syntax": "^0.4.0",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^15.10.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "terser": "^5.18.2",
    "tinymce": "^6.5.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5.1.6",
    "webamp": "^1.5.0",
    "xlsx": "^0.18.5",
    "xterm": "^5.2.1"
  }
}
